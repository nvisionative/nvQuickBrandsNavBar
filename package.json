{
  "name": "@nvisionative/nvq-brands-nav-bar",
  "version": "1.2.0-beta.1",
  "description": "nvQuickBrandsNavBar is a Stencil component that provides a navigation bar for multi-brand web properties.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/nvq-brands-nav-bar/nvq-brands-nav-bar.esm.js",
  "exports": {
    ".": {
      "import": "./dist/nvq-brands-nav-bar/nvq-brands-nav-bar.esm.js",
      "require": "./dist/nvq-brands-nav-bar/nvq-brands-nav-bar.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nvisionative/nvQuickBrandsNavBar.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "npm run lint && stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@stencil/eslint-plugin": "^1.0.0",
    "@stencil/sass": "^3.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jiti": "^2.4.2",
    "puppeteer": "^24.3.0",
    "typescript-eslint": "^8.31.0"
  },
  "license": "MIT"
}
